{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext",
      "webworker"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true,
    "baseUrl": ".",
    "paths": {
      "@/appContext/*": ["src/appContext/*"],
      "@/components/*": ["src/components/*"],
      "@/db/*": ["src/db/*"],
      "@/config/*": ["src/config/*"],
      "@/hooks/*": ["src/hooks/*"],
      "@/pages/*": ["src/pages/*"],
      "@/public/*": ["public/*"],
      "@/reduxStore/*": ["src/reduxStore/*"],
      "@/remoteAPI/*": ["src/remoteAPI/*"],
      "@/styles/*": ["src/styles/*"],
      "@/types/*": ["src/types/*"],
      "@/ui/*": ["src/ui/*"],
      "@/utils/*": ["src/utils/*"],
      "@/YPTechHTTPClient/*": ["src/YPTechHTTPClient/*"],
    }
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx"],
  "exclude": ["node_modules"],
}
